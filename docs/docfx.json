{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Agibuild.Fulora.Core/Agibuild.Fulora.Core.csproj",
            "src/Agibuild.Fulora.Runtime/Agibuild.Fulora.Runtime.csproj",
            "src/Agibuild.Fulora/Agibuild.Fulora.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "properties": {
        "TargetFramework": "net10.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "Agibuild.Fulora",
      "_appName": "Agibuild.Fulora",
      "_appFooter": "Agibuild.Fulora â€” Cross-platform Hybrid App Framework",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}
